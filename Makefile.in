	bootos_linux.cpp \
	cdrom.cpp \
	cfgopts.cpp \
	dirty_rects.cpp \
	dsp.cpp \
	emul_op.cpp \
	natfeats.cpp \
	fakeio.cpp \
	hardware.cpp \
	host.cpp \
	host_audio.cpp \
	host_clock.cpp \
	hostscreen.cpp \
	hostscreen_opengl.cpp \
	host_surface.cpp \
	host_surface_opengl.cpp \
	ide.cpp \
	identify.cpp \
	ikbd.cpp \
	input.cpp \
	logo.cpp \
